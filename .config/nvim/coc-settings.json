{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "json",
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "diagnostic.checkCurrentLine": true,
  "diagnostic.displayByAle": false,
  "diagnostic.enableMessage": "jump",
  "diagnostic.level": "information",
  "diagnostic.messageTarget": "echo",
  "explorer.icon.enableNerdfont": true,
  "explorer.width": 40,
  "explorer.contentWidth": 50,
  "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5][size]",
  "highlight.colorNames.enable": false,
  "prettier.disableLanguages": [],
  "stylelintplus.autoFixOnFormat": true,
  "stylelintplus.autoFixOnSave": true,
  "volar.formatting.enable": false
}
